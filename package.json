{
  "name": "modular-css",
  "version": "0.0.0",
  "private": true,
  "author": "Pat Cavit <npm@patcavit.com>",
  "homepage": "https://m-css.com",
  "license": "MIT",
  "repository": "tivac/modular-css",
  "bugs": {
    "url": "https://github.com/tivac/modular-css/issues"
  },
  "scripts": {
    "cover": "jest --coverage",
    "lint": "eslint .",
    "outdated": "lerna exec -- node ../../lerna/outdated.js",
    "parsers": "node parsers/build.js",
    "release": "env-cmd lerna publish",
    "prepublishOnly": "npm run parsers",
    "pretest": "npm run parsers",
    "test": "jest",
    "watch": "jest --watch",
    "www:build": "npm run build --workspace=www",
    "www:start": "npm start --workspace=www",
    "prewww:deploy": "npm run parsers",
    "www:deploy": "npm run deploy --workspace=www"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@tivac/eslint-config": "^2.4.0",
    "cli-tester": "2.0.0",
    "dedent": "0.7.0",
    "dentist": "1.0.3",
    "env-cmd": "^10.0.1",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "lerna": "^4.0.0",
    "node-notifier": "^10.0.0",
    "peggy": "^1.2.0",
    "postcss": "^8.4.5",
    "rollup": "^2.66.1",
    "shelljs": "^0.8.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
